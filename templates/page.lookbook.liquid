<div class="row">
  <div class="col-xs-12"><h10>Vanessa Lianne Jewelry is featured in: Vogue, WWD, Lucky, Globe &amp; Mail, NY Mag, &amp; Martha Stewart Living</h10>
  </div>
</div>

<!-- Begin slider -->
<div class="col-xs-12">
  <div class="flexslider-container lookbook-flexslider">
    <div class="flexslider">
      <ul class="slides pull-center">
        {% for i in (1..10) %}
          {% capture show_slide %}display_slide_{{ i }}{% endcapture %}
          {% capture image %}slideshow_{{ i }}.jpg{% endcapture %}
          {% capture link %}slide_{{ i }}_link{% endcapture %}
          {% capture alt %}slide_{{ i }}_alt{% endcapture %}
          {% if settings[show_slide] %}
          <li>
            <a href="http://gromn.myshopify.com/products/white-tank-1" target="blank">
              <img src="https://cdn.shopify.com/s/files/1/0271/2135/t/2/assets/lookbook-page1.jpg?12626" />
              <p>Shop this collection →</p>
            </a>
          </li>          
          <li>
            <a href="http://gromn.myshopify.com/products/white-tank-1" target="blank">
              <img src="https://cdn.shopify.com/s/files/1/0271/2135/t/2/assets/lookbook-page1.jpg?12626" />
              <p>Shop this collection →</p>
            </a>
          </li>
          {% endif %}
        {% endfor %}
      </ul>
      <div class="flex-controls"></div>
    </div>
  </div>
</div>


<!-- End slider -->